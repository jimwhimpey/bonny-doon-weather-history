	console.log('days', {{{daysJson}}});
	console.log('months', {{{monthsJson}}});
</script>

{{#each months}}

	<h1>{{momentFormat this.date "MMM"}}</h1>

	<table>
		<tr>
			<th>&nbsp;</th>
			<th>Low</th>
			<th>High</th>
			<th>Rain Probability</th>
			<th>Rain Intensity</th>
			<th>Rain Intensity Max</th>
			<th>Humidity</th>
			<th>Clouds</th>
			<th>Dew</th>
			<th>Pressure</th>
			<th>Ozone</th>
		</tr>

		{{#each this.days}}
			<tr>
				<td>{{momentFormat this.dateMoment "Do"}}</td>
				<td>{{temperatureF this.data.apparentTemperatureLow}}</td>
				<td>{{temperatureF this.data.apparentTemperatureHigh}}</td>
				<td>{{percentage this.data.precipProbability}}</td>
				<td>{{intensity this.data.precipIntensity}}</td>
				<td>{{intensity this.data.precipIntensityMax}}</td>
				<td>{{percentage this.data.humidity}}</td>
				<td>{{percentage this.data.cloudCover}}</td>
				<td>{{temperatureF this.data.dewPoint}}</td>
				<td>{{pressure this.data.pressure}}</td>
				<td>{{this.data.ozone}}</td>
			</tr>
		{{/each}}

	</table>

{{/each}}

